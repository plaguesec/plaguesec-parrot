# Launcher icons
[org.gnome.shell]
favorite-apps=['install-debian.desktop', 'firefox.desktop', 'libreoffice-writer.desktop', 'org.gnome.Nautilus.desktop', 'codium.desktop']

# We don't want the screensaver to enable on the live media while installing
[org.gnome.desktop.lockdown]
disable-lock-screen=true

[org.gnome.desktop.session]
idle-delay=0
